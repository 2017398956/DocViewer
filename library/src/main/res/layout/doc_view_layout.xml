<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <com.cherry.lib.doc.ProgressWebView
        android:id="@+id/mPWeb"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>

    <FrameLayout
        android:id="@+id/mFlDocContainer"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>

    <com.cherry.lib.doc.pdf.PdfRendererView
        android:id="@+id/mPdfView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:pdfView_show_page_num="true"
        app:pdfView_showDivider="true"
        app:pdfView_page_pb_height="2dp"/>

    <ImageView
        android:id="@+id/mIvImage"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:scaleType="fitCenter"/>

</FrameLayout>